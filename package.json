{
  "name": "typescript-sql-playground",
  "type": "module",
  "version": "0.0.0",
  "description": "Project for exploring SQL query builders.",
  "author": "Tramaine L. Gillus",
  "repository": {
    "type": "git",
    "url": "git@github.com:tgillus/typescript-sql-playground.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "npm run clean && npm test && tsc",
    "clean": "del-cli lib coverage",
    "db:drizzle:generate": "drizzle-kit generate:sqlit",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepack": "npm run build",
    "prepare": "husky install",
    "release": "release-it --ci",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "watch": "tsc -c"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@release-it/conventional-changelog": "8.0.1",
    "@types/better-sqlite3": "7.6.10",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "7.7.1",
    "@typescript-eslint/parser": "7.7.1",
    "@vitest/coverage-v8": "1.5.2",
    "del-cli": "5.1.0",
    "drizzle-kit": "0.20.17",
    "eslint": "8.57.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "prisma": "5.13.0",
    "release-it": "17.2.1",
    "testdouble": "3.20.2",
    "tsx": "4.7.3",
    "typescript": "5.4.5",
    "vitest": "1.5.2"
  },
  "dependencies": {
    "@prisma/client": "5.13.0",
    "better-sqlite3": "9.6.0",
    "drizzle-orm": "0.30.9",
    "kysely": "0.27.3",
    "sequelize": "6.37.3",
    "sqlite3": "5.1.7"
  }
}
