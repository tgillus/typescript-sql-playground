{
  "author": "Tramaine L. Gillus",
  "dependencies": {
    "@prisma/client": "6.1.0",
    "better-sqlite3": "11.7.0",
    "drizzle-orm": "0.38.2",
    "kysely": "0.27.5",
    "sequelize": "6.37.5",
    "sqlite3": "5.1.7"
  },
  "description": "Project for exploring SQL query builders.",
  "devDependencies": {
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@eslint/js": "9.17.0",
    "@release-it/conventional-changelog": "9.0.3",
    "@types/better-sqlite3": "7.6.12",
    "@types/eslint__js": "8.42.3",
    "@types/node": "22.10.2",
    "@vitest/coverage-v8": "2.1.8",
    "del-cli": "6.0.0",
    "drizzle-kit": "0.30.1",
    "eslint": "9.17.0",
    "globals": "15.14.0",
    "husky": "9.1.7",
    "lint-staged": "15.2.11",
    "prettier": "3.4.2",
    "prisma": "6.1.0",
    "release-it": "17.10.0",
    "testdouble": "3.20.2",
    "tsx": "4.19.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.1",
    "vitest": "2.1.8"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "name": "typescript-sql-playground",
  "repository": {
    "type": "git",
    "url": "git@github.com:tgillus/typescript-sql-playground.git"
  },
  "scripts": {
    "build": "npm run clean && npm test && tsc",
    "clean": "del-cli lib coverage",
    "db:drizzle:generate": "drizzle-kit generate:sqlit",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepack": "npm run build",
    "prepare": "husky",
    "release": "release-it --ci",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "watch": "tsc -c"
  },
  "type": "module",
  "version": "0.0.0"
}
