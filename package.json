{
  "name": "typescript-sql-playground",
  "type": "module",
  "version": "0.0.0",
  "description": "Project for exploring SQL query builders.",
  "author": "Tramaine L. Gillus",
  "repository": {
    "type": "git",
    "url": "git@github.com:tgillus/typescript-sql-playground.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "npm run clean && npm test && tsc",
    "clean": "del-cli lib coverage",
    "db:drizzle:generate": "drizzle-kit generate:sqlit",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepack": "npm run build",
    "prepare": "husky install",
    "release": "release-it --ci",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "watch": "tsc -c"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.2",
    "@commitlint/config-conventional": "17.7.0",
    "@release-it/conventional-changelog": "7.0.2",
    "@types/better-sqlite3": "7.6.5",
    "@types/node": "20.8.4",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "@vitest/coverage-v8": "0.34.6",
    "del-cli": "5.1.0",
    "drizzle-kit": "0.19.13",
    "eslint": "8.51.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "prisma": "5.4.2",
    "release-it": "16.2.1",
    "testdouble": "3.19.0",
    "tsx": "3.13.0",
    "typescript": "5.2.2",
    "vitest": "0.34.6"
  },
  "dependencies": {
    "@prisma/client": "5.4.2",
    "better-sqlite3": "8.7.0",
    "drizzle-orm": "0.28.6",
    "kysely": "0.26.3",
    "sequelize": "6.33.0",
    "sqlite3": "5.1.6"
  }
}
